{
  "[handlebars]": {
    "editor.suggest.insertMode": "replace",
    "editor.formatOnSave": true
  },
  "[markdown]": {
    "editor.wordWrap": "on",
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    }
  },
  "[python]": {
    "editor.tabSize": 4
  },
  "window.title": "ðŸ¦•",
  "window.menuBarVisibility": "toggle",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.colorCustomizations": {
    "[Tokyo Night]": {
      "editor.foreground": "#e5e6e7",
      "terminal.foreground": "#e5e6e7"
    },
    "tab.activeBorderTop": "#95dba7",
    "tab.activeBackground": "#2c302c",
    "tab.activeForeground": "#fff",
    "tab.inactiveModifiedBorder": "#bf616a",
    "tab.activeModifiedBorder": "#bf616a"
  },
  "editor.formatOnPaste": true,
  "editor.minimap.enabled": false,
  "editor.formatOnSave": true,
  "editor.tabSize": 4,
  "editor.tabCompletion": "on",
  "css.validate": true,
  "explorer.sortOrder": "type",
  "editor.suggestSelection": "first",
  "editor.cursorBlinking": "solid",
  "terminal.external.linuxExec": "x-terminal-emulator",
  "terminal.integrated.env.linux": {
    "VSCODE_INTEGRATED_TERMINAL": "true"
  },
  "editor.fontFamily": "Hack, 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.fontLigatures": false,
  "terminal.integrated.tabs.enabled": true,
  "workbench.startupEditor": "none",
  "explorer.confirmDelete": false,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "keyboard.dispatch": "keyCode",
  "editor.fontSize": 14,
  "vim.useSystemClipboard": true,
  "editor.cursorSmoothCaretAnimation": "explicit",
  "explorer.confirmDragAndDrop": false,
  "vim.cursorStylePerMode.insert": "line",
  "editor.emptySelectionClipboard": false,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  // Declutter
  "workbench.sideBar.location": "right",
  // "workbench.activityBar.location": "hidden",
  "workbench.statusBar.visible": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.overviewRulerBorder": true,
  "editor.scrollbar.horizontal": "auto",
  "editor.scrollbar.vertical": "auto",
  "workbench.tree.renderIndentGuides": "none",
  "explorer.openEditors.visible": 0,
  "search.decorations.colors": false,
  "markdown.preview.fontFamily": "Open Sans, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', system-ui, 'Ubuntu', 'Droid Sans', sans-serif",
  "markdown.preview.fontSize": 16,
  "editor.lineHeight": 1.7,
  "terminal.integrated.lineHeight": 1.2,
  "markdown.preview.lineHeight": 1.8,
  "cSpell.userWords": [
    "aluminium",
    "atonalness",
    "Banquo",
